{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "rmse-vat-results/schedule",
  "version": "1.0.0",
  "title": "Team Schedule",
  "category": "widgets",
  "icon": "calendar",
  "description": "Show the schedule and last results of a team",
  "supports": {
    "align": true
  },
  "attributes": {
    "team": {
      "type": "string",
      "default": ""
    },
    "header": {
      "type": "boolean",
      "default": true
    },
    "venue": {
      "type": "boolean",
      "default": true
    },
    "type": {
      "type": "boolean",
      "default": true
    },
    "with_result": {
      "type": "boolean",
      "default": true
    },
    "dateformat": {
      "type": "string",
      "default": "d.m.y H:i"
    }
  },
  "textdomain": "rmse-vat-results",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}